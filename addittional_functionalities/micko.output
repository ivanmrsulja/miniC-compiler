   50 otherwise: %empty

   51 $@8: %empty

   52 otherwise: _OTHERWISE _COLON $@8 statement

   53 $@9: %empty

   54 para_statement: _PARA _LPAREN _ID _ASSIGN literal _EN literal _RPAREN $@9 statement

   55 $@10: %empty

   56 select_statement: _SELECT $@10 vars _FROM _ID _WHERE _LPAREN rel_exp _RPAREN _SEMICOLON

   57 $@11: %empty

   58 $@12: %empty

   59 compound_statement: _LBRACKET $@11 variable_list $@12 statement_list _RBRACKET

   60 $@13: %empty

   61 assignment_statement: _ID $@13 _ASSIGN num_exp _SEMICOLON

   62 $@14: %empty

   63 assignment_statement: indexing $@14 _ASSIGN num_exp _SEMICOLON

   64 indexing: _ID _LSQBRACKET exp _RSQBRACKET

   65 num_exp: exp
   66        | num_exp _AROP exp

   67 conditional: _LPAREN rel_exp _RPAREN _QMARK half_exp _COLON half_exp

   68 exp: half_exp
   69    | full_exp

   70 half_exp: literal
   71         | _ID

   72 full_exp: function_call
   73         | _LPAREN num_exp _RPAREN
   74         | unaryop
   75         | conditional
   76         | indexing

   77 unaryop: _ID _UNOP
   78        | indexing _UNOP

   79 literal: _INT_NUMBER
   80        | _UINT_NUMBER

   81 $@15: %empty

   82 function_call: _ID $@15 _LPAREN argument_list _RPAREN

   83 argument_list: %empty
   84              | arguments

   85 arguments: argument
   86          | arguments _COMMA argument

   87 argument: num_exp

   88 if_statement: if_part
   89             | if_part _ELSE statement

   90 @16: %empty

   91 $@17: %empty

   92 if_part: _IF _LPAREN @16 rel_exp $@17 _RPAREN statement

   93 rel_exp: num_exp _RELOP num_exp

   94 return_statement: _RETURN num_exp _SEMICOLON
   95                 | _RETURN _SEMICOLON


